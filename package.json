{
  "name": "sgn-tpl",
  "version": "1.0.0",
  "description": "this a angualr framework template",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "dev": "cross-env NODE_ENV=DEV webpack-dev-server --progress",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=PRO webpack -p --colors",
    "postbuild": "ncp ./dll/dll.angular.js ./dist/dll.angular.js && ncp ./dll/dll.angular.js.map ./dist/dll.angular.js.map ",
    "build:dll": "webpack -p --config=webpack.dll.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "angular",
    "template"
  ],
  "author": "sean",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.6.2",
    "angular-ui-bootstrap": "^2.5.0",
    "angular-ui-router": "^0.4.2",
    "bootstrap-sass": "^3.3.7",
    "oclazyload": "^1.1.0"
  },
  "devDependencies": {
    "@types/angular": "^1.6.7",
    "@types/angular-ui-bootstrap": "^0.13.40",
    "@types/angular-ui-router": "^1.1.36",
    "@types/node": "^7.0.5",
    "@types/oclazyload": "0.0.31",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.0.7",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-webpack-include-assets-plugin": "0.0.4",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-browser-plugin": "^1.0.20",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  }
}
