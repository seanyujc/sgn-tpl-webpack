{
  "name": "sgn-tpl",
  "version": "1.0.0",
  "description": "this a angualr framework template",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist ./dist/assets ./dist/dll",
    "cpdll": "ncp ./dll/angular-dll.js ./dist/dll/angular-dll.js && ncp ./dll/angular-dll.js.map ./dist/dll/angular-dll.js.map && ncp ./dll/plugins-dll.js ./dist/dll/plugins-dll.js && ncp ./dll/plugins-dll.js.map ./dist/dll/plugins-dll.js.map && ncp ./dll/assets-dll.js ./dist/dll/assets-dll.js && ncp ./dll/assets-dll.js.map ./dist/dll/assets-dll.js.map && ncp ./dll/bootstrap.css ./dist/bootstrap.css && ncp ./dll/bootstrap.css.map ./dist/bootstrap.css.map && ncp ./dll/dev-dll.js ./dist/dll/dev-dll.js && ncp ./dll/dev-dll.js.map ./dist/dll/dev-dll.js.map",
    "cpassets": "ncp ./src/assets/images ./dist/assets/images && ncp ./src/_upload ./dist/_upload",
    "dev": "cross-env NODE_ENV=DEV cross-env PUBLIC_PATH=/  webpack-dev-server --progress",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=PRO webpack -p --colors",
    "postbuild": "npm run cpdll && npm run cpassets",
    "prebuild:test": "npm run prebuild",
    "build:test": "cross-env NODE_ENV=TEST webpack -p --colors",
    "postbuild:test": "npm run postbuild",
    "prebuild:dev": "npm run prebuild",
    "build:dev": "cross-env NODE_ENV=DEV webpack -p --colors",
    "postbuild:dev": "npm run postbuild",
    "prebuild:dll": "rimraf dll",
    "build:dll": "webpack -p --config=webpack.dll.js",
    "postbuild:dll": "npm run cpdll",
    "preinstall": "npm i rimraf mkdirp -g && npm run clean",
    "postinstall": "npm run build:dll && npm run cpassets",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "angular",
    "template"
  ],
  "author": "sean",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.6.3",
    "angular-animate": "^1.6.3",
    "angular-i18n": "^1.6.3",
    "angular-iscroll": "^3.4.0",
    "angular-sanitize": "^1.6.3",
    "angular-touch": "^1.6.4",
    "angular-ui-bootstrap": "^2.5.0",
    "angular-ui-router": "^0.4.2",
    "bootstrap-sass": "^3.3.7",
    "ng-bases": "^1.1.12",
    "oclazyload": "^1.1.0"
  },
  "devDependencies": {
    "@types/angular": "^1.6.7",
    "@types/angular-animate": "^1.5.6",
    "@types/angular-ui-bootstrap": "^0.13.40",
    "@types/angular-ui-router": "^1.1.36",
    "@types/lodash": "^4.14.54",
    "@types/node": "^7.0.5",
    "@types/oclazyload": "0.0.31",
    "@types/webpack-env": "^1.13.0",
    "angular-mocks": "^1.6.4",
    "awesome-typescript-loader": "^3.0.7",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "happypack": "^3.0.3",
    "html-loader": "^0.4.5",
    "html-webpack-include-assets-plugin": "0.0.4",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "sass-loader": "^6.0.2",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.13.2",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-browser-plugin": "^1.0.20",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  }
}
