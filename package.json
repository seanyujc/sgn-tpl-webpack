{
  "name": "sgn-tpl",
  "version": "1.0.0",
  "description": "this a angualr framework template",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist ./dist/assets ./dist/dll",
    "cpdll": "ncp ./dll/dll/ ./dist/dll/ && ncp ./dll/styles/ ./dist/styles/ && ncp ./dll/assets/ ./dist/assets/",
    "cpassets": "ncp ./src/assets/images ./dist/assets/images",
    "dev": "node ./build/dev-server.js",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=PRO node ./build/build.js",
    "postbuild": "npm run cpdll && npm run cpassets",
    "prebuild:dev": "npm run prebuild",
    "build:dev": "cross-env NODE_ENV=DEV node ./build/build.js",
    "postbuild:dev": "npm run postbuild",
    "prebuild:test": "npm run prebuild",
    "build:test": "cross-env NODE_ENV=TEST node ./build/build.js",
    "postbuild:test": "npm run postbuild",
    "prebuild:dll": "rimraf dll",
    "build:dll": "webpack -p --config=webpack.dll.js",
    "postbuild:dll": "npm run cpdll",
    "preinstall": "npm config set msvs_version 2012 --global && npm i cross-env ncp rimraf mkdirp typescript tslint eslint -g && npm run clean",
    "postinstall": "npm run build:dll && npm run cpassets",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "angular",
    "template"
  ],
  "author": "sean",
  "license": "ISC",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.6",
    "angular": "^1.6.6",
    "angular-animate": "^1.6.6",
    "angular-i18n": "^1.6.6",
    "angular-iscroll": "^3.5.1",
    "angular-sanitize": "^1.6.6",
    "angular-touch": "^1.6.6",
    "angular-ui-bootstrap": "^2.5.0",
    "bootstrap-sass": "^3.3.7",
    "oclazyload": "^1.1.0"
  },
  "devDependencies": {
    "@types/angular": "^1.6.7",
    "@types/angular-animate": "^1.5.6",
    "@types/angular-ui-bootstrap": "^0.13.40",
    "@types/angular-ui-router": "^1.1.36",
    "@types/lodash": "^4.14.54",
    "@types/node": "8.0.26",
    "@types/oclazyload": "0.0.31",
    "@types/webpack-env": "1.13.0",
    "angular-mocks": "^1.6.4",
    "awesome-typescript-loader": "^3.0.7",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "bootstrap-loader": "^2.1.0",
    "chalk": "^2.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "happypack": "^3.0.3",
    "html-loader": "^0.4.5",
    "html-webpack-include-assets-plugin": "0.0.4",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "ora": "^1.3.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.2",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.13.2",
    "ts-loader": "^2.0.1",
    "typescript": "^2.5.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "url-loader": "^0.5.8",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
